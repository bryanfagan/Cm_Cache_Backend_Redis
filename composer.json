{
    "name": "colinmollenhour/cache-backend-redis",
    "keywords": ["redis", "caching", "zf", "zend framework"],
    "license": "BSD-3-Clause",
    "homepage": "https://github.com/bryanfagan/Cm_Cache_Backend_Redis",
    "description": "Zend_Cache backend using Redis with full support for tags.",
    "authors": [
        {
            "name": "Colin Mollenhour",
            "email": "colin@mollenhour.com"
        }
    ],
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/chrisboulton/credis"
        }
    ],
    "require": {
        "php": ">=5.3.0",
        "zendframework/zendframework1": "1.12.3",
        "colinmollenhour/credis": "1.2.*"
    },
    "suggest": {
        "ext-redis": "Native PHP extension for Redis connectivity. Credis will automatically utilize when available."
    },
    "require-dev": {
        "phpunit/phpunit": "3.7.*"
    },
    "autoload": {
        "psr-0": { "CM": "." }
    }
}
